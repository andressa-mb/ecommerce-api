{
    "version": 2,
    "builds": [
        {
            "src": "api.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api.js"
        }
    ],
    "headers": [
        {
            "source": "/",
            "headers": [
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src 'self'; script-src 'self' /api.js; style-src 'self'"
                }
            ]
        }
    ]
}
